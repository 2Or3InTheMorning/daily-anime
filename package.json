{
	"name": "daily-anime",
	"displayName": "daily-anime",
	"description": "a extension for daily anime",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.dailyAnime"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.dailyAnime",
				"title": "每日番剧",
				"category": "anime"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"build:sass": "gulp",
		"view": "http-server ./src/template/ -p 4444 -c-1"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"cssnano": "^4.1.10",
		"gulp": "^4.0.0",
		"gulp-sass": "^4.0.2",
		"http-server": "^0.11.1",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"axios": "^0.18.0"
	}
}
